test LagkJointMomentsFromMAP
inputmsg
defmat@ D0 # {{-5, 0, 1, 1},{1, -8, 1, 0},{1, 0, -4, 1},{1, 2, 3, -9}}
defmat@ D1 # {{0, 1, 0, 2},{2, 1, 3, 0},{0, 0, 1, 1},{1, 1, 0, 1}}
testmsg
code!@ Nm=LagkJointMomentsFromMAP[D0,D1,4,1]
code!@ moms=MarginalMomentsFromMAP[D0,D1,4]
CODEP cjm=np.empty(3)
CODEP for i in range(3):
CODEP     Nx=LagkJointMomentsFromMAP(D0,D1,1,i+1)
CODEP     cjm[i] = (Nx[1,1]-moms[0]**2) / (moms[1]-moms[0]**2)
CODEM cjm=zeros(3,1);
CODEM for i=1:3
CODEM     Nx=LagkJointMomentsFromMAP(D0,D1,1,i);
CODEM     cjm(i) = (Nx(2,2)-moms(1)^2) / (moms(2)-moms(1)^2);
CODEM end
print "Correlation from joint moments:" # cjm
code!@ corr = LagCorrelationsFromMAP[D0,D1,3]
assert AllPositiveMatrix[Nm] && Norm[NPArray[moms]-Nm[[1,2;;]]]<10^-14 && Norm[NPArray[moms]-Nm[[2;;,1]]]<10^-14 && Norm[corr-cjm]<10^-14 # Joint moment matrix is invalid!

