>>> A = ml.matrix([[-1, 0, 0],[0, -3, 0.6],[0, -0.6, -3]])
>>> B = TransformToMonocyclic(A)
>>> print(B)
[[-1.       1.       0.       0.     ]
 [ 0.      -2.65359  2.65359  0.     ]
 [ 0.       0.      -2.65359  2.65359]
