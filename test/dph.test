test PmfFromMG

inputmsg
defvec@ a # {-0.6, 0.3, 1.3}
defmat@ A # {{0.25, 0.2, -0.15},{0.3, 0.1, 0.25},{0, 0.2, 0.47}}
defrange x # 0 # 100 # 1
testmsg    
code! pmf=PmfFromMG[a,A,x]
plot x # pmf
assertW AllTrue[pmf,NonNegative] # PmfFromMG returned negative pmf!
assertMP All[pmf>=0] # PmfFromMG returned negative pmf!
assertW Abs[pmf.x - MomentsFromMG[a,A,1][[1]]]<10^-10 # The mean computed from the pmf does not match the theoretical result!
assertM Abs[x*pmf - MomentsFromMG[a,A,1][[1]]]<10^-10 # The mean computed from the pmf does not match the theoretical result!
assertP Abs[Dot[pmf,x] - MomentsFromMG[a,A,1][[1]]]<10^-10 # The mean computed from the pmf does not match the theoretical result!
    
