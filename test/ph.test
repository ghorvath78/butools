test CdfFromME

inputmsg
defvec@ a # {0.2, 0.3, 0.5}
defmat@ A # {{-1,0,0},{0,-3,2},{0,-2,-3}}
defrange x # 0 # 5 # 0.01
testmsg
code! cdf = CdfFromME[a,A,x]
plot x # cdf
assertW AllTrue[Differences[cdf],Positive] # The cdf is not increasing monotonously!
assertMP All[Diff[cdf]>0] # The cdf is not increasing monotonously!
assert Abs[Total[1-cdf]*0.01 - MomentsFromME[a,A,1][[1]]]<0.01 # The mean computed from the cdf does not match the theoretical result!

