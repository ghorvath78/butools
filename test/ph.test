test PdfFromME

inputmsg
defvec@ a # {0.2, 0.3, 0.5}
defmat@ A # {{-1,0,0},{0,-3,2},{0,-2,-3}}
defrange x # 0 # 5 # 0.01
testmsg
code! pdf = PdfFromME[a,A,x]
plot x # pdf
assertW AllTrue[pdf,NonNegative] # The pdf is negative!
assertMP All[pdf>=0] # The pdf is negative!
assertW Abs[pdf.x*0.01 - MomentsFromME[a,A,1][[1]]]<0.01 # The mean computed from the pdf does not match the theoretical result!
assertM Abs[x*pdf*0.01 - MomentsFromME[a,A,1][[1]]]<0.01 # The mean computed from the pdf does not match the theoretical result!
assertP Abs[Dot[pdf,x]*0.01 - MomentsFromME[a,A,1][[1]]]<0.01 # The mean computed from the pdf does not match the theoretical result!

